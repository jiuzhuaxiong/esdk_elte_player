<?xml version="1.0" encoding="utf-8"?>
<dao:DAOCfgs xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:dao="http://www.td-tech.com/ubp/platform/dao"
             xsd:schemaLocation="http://www.td-tech.com/ubp/platform/dao ubp_db_cfgdef.xsd">

  <DAO name="mdc_dao"
       active="true"
       DefaultDAO="true">
    <dbcfg name="user"
           value="$ubp_odb_user" />
    <dbcfg name="passwd"
           value="$ubp_odb_psw" />
    <dbcfg name="database"
           value="$ubp_db_name" />
    <!--dbcfg name="host"
           value="$ubp_local_ip" /-->
    <dbcfg name="port"
           value="3306" />
    <dbcfg name="max_connection"
           value="30" />
    <dbcfg name="min_connection"
           value="10" />
    <MOC name="PTTUser" />
    <MOC name="FixedUser" />
    <MOC name="DCAttribute" />
    <MOC name="Groups" />
    <MOC name="GroupPTTUser" />
    <MOC name="GroupFixedUser" />
    <MOC name="VPN" />
    <MOC name="VPNRange" />
    <MOC name="UBPNode" />
    <MOC name="BccNeighborNode" />
    <MOC name="BccCfg" />
    <MOC name="MrsRange" />
    <MOC name="RecRangeList" />
    <MOC name="DynamicGroup" />
    <MOC name="DynamicGroupMember" />
    <MOC name="DMGPhotoGroupUser" />
    <MOC name="CallInhibited" />
    <MOC name="DCRanges" />
    <MOC name="RangeList" />
    <MOC name="MRSVPN" />
  </DAO>
  <DAO name="mrs_dao"
       active="false"
       DefaultDAO="false">
    <dbcfg name="user"
           value="$ubp_odb_user" />
    <dbcfg name="passwd"
           value="$ubp_odb_psw" />
    <dbcfg name="database"
           value="$ubp_db_name" />
    <!--dbcfg name="host"
           value="$ubp_local_ip" /-->
    <dbcfg name="port"
           value="3306" />
    <dbcfg name="max_connection"
           value="30" />
    <dbcfg name="min_connection"
           value="10" />
    <!-- following MOC and operated by this DAO-->
    <MOC name="CallRecTaskInfo" />
    <MOC name="RecFile" />
    <MOC name="RecSipInfo" />
    <MOC name="RecFileInfo" />
    <MOC name="RecPttInfo" />
    <MOC name="RecPremptedInfo" />
    <MOC name="MRSNode" />
    <MOC name="BCCInfo" />
    <MOC name="IpcDevice" />
    <MOC name="ScheduleRecTask" />
    <MOC name="MRSFileInfo" />
    <MOC name="IpcRecTaskInfo" />
    <MOC name="SipCallRecQuery" />
    <MOC name="IpcRecQuery" />
    <MOC name="SipGroupCallRecQuery" />
    <MOC name="MRSRecRangeList" />
    <MOC name="MRSSipRecScheduleTask" />
  </DAO>
</dao:DAOCfgs>
