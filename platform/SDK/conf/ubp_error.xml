-->

<Errors xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
       xsd:noNamespaceSchemaLocation="error_number.xsd">

<!--
//////////////////////////////////////////////////////////////////////////
//ErrorInfo for UBP common : 0 ~ - 9999
//////////////////////////////////////////////////////////////////////////
-->
  <Error>
		<code>0</code>
		<detail message="success" suggestion="success"/>
	</Error>
	<Error>
		<code>-1</code>
		<detail message="failed" suggestion=""/>
