<?xml version="1.0" encoding="utf-8" ?>
<Moc-Config>
	<MOC MOC_NAME="RecFileInfo" Module="MRS" ACCESS_CONTROL="add,delete,modify,read" DEST_SVC="only" FILTER="">
		<Parameter name="file_id_info_task_id" desc="Task ID" type="int" range="" constrain="PRI" mode="display" />
		<Parameter name="file_id_info_file_id" desc="File ID" type="int" range="" constrain="PRI" mode="display" />
		<Parameter name="file_name" desc="File path" type="string" range="[1,250]" mode="display" />
		<Parameter name="file_status" desc="File status" type="enum" range="0,1,2,3,4,5" vlist="0: CREATED (creation success),1: FAIL (storage failure),2: RECEIVING (being stored),3: SUCCESS (success),4：WATITO_DELETEING (waiting to be deleted),5: DELETED (deletion success)" mode="display" />
		<Parameter name="file_ref_num" desc="Count of reference files" type="int" range="[0,65535]" mode="display" />
	</MOC>
</Moc-Config>